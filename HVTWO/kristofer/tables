create table movies (
id varchar(250) unique,
title varchar(250),
year varchar(250),
rating float
);

create table ratings (
movieid varchar(250) references movies(id),
userid varchar(250),
rating float,
primary key (movieid, userid)
);

create table links (
movieid varchar(250) references movies(id) unique,
imdbid varchar(250),
tmdbid varchar(250)
);

create table tags (
movieid varchar(250) references movies(id),
userid varchar(250),
tag varchar(250),
primary key (movieid, userid, tag)
);

create table mtags (
movieid varchar(250) references movies(id),
tag varchar(250),
primary key (movieid, tag)
);

create table genres (
genre varchar(250),
movieid varchar(250) references movies(id),
primary key (genre, movieid)
);